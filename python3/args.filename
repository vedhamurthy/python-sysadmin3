<script>
  $(document).ready(function() {

    var screenWidth = $( window ).width();

    toggleColumn(screenWidth);
    
    $( window ).resize(function() {
      screenWidth = $( window ).width();
      toggleColumn(screenWidth);
    });

  });

  function toggleColumn(screenWidth) {
    if (screenWidth < 504) {
      $('.bg-image-lines').children('.col-xs-8').addClass('col-xs-offset-1').removeClass('col-xs-offset-2');
    } else if (screenWidth > 504) {
      $('.bg-image-lines').children('.col-xs-8').addClass('col-xs-offset-2').removeClass('col-xs-offset-1');
    }
  }

  //Initialize wow.js
  new WOW().init();
</script>

